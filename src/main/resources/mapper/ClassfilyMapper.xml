<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.id.mapper.ClassfilyMapper">
    <select id="getListClassfily" resultType="java.util.HashMap">
        select * from classfily
    </select>

    <insert id="insertOneClassfily">
        insert into `classfily`(classfilyname) VALUE (#{classfilyname})
    </insert>

    <delete id="deleteOneClassfily">
        delete from `classfily` where classfilyname = #{classfilyname}
    </delete>
</mapper>